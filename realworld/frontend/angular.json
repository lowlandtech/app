{
  "version": 1,
  "projects": {
    "spotacard": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/spotacard",
      "sourceRoot": "apps/spotacard/src",
      "prefix": "spotacard",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/spotacard",
            "index": "apps/spotacard/src/index.html",
            "main": "apps/spotacard/src/main.ts",
            "polyfills": "apps/spotacard/src/polyfills.ts",
            "tsConfig": "apps/spotacard/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/spotacard/src/favicon.ico",
              "apps/spotacard/src/assets"
            ],
            "styles": ["apps/spotacard/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/spotacard/src/environments/environment.ts",
                  "with": "apps/spotacard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "spotacard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "spotacard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "spotacard:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/spotacard/tsconfig.app.json",
              "apps/spotacard/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/spotacard/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/spotacard/jest.config.js",
            "tsConfig": "apps/spotacard/tsconfig.spec.json",
            "setupFile": "apps/spotacard/src/test-setup.ts"
          }
        }
      }
    },
    "spotacard-e2e": {
      "root": "apps/spotacard-e2e",
      "sourceRoot": "apps/spotacard-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spotacard-e2e/cypress.json",
            "tsConfig": "apps/spotacard-e2e/tsconfig.e2e.json",
            "devServerTarget": "spotacard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "spotacard:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/spotacard-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/spotacard-e2e/**"]
          }
        }
      }
    },
